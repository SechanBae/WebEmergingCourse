{{>header}}

{{>navigation}}

<div class="row">
  <div class="leftcolumn">
    <h2>Editors</h2>
    <h4>Users</h4>
    <table>
      <thead>
        <tr>
          <th>Username</th>
          <th>Password</th>
          <th>Level</th>
          <th>Delete</th>
        </tr>
      </thead>
      <tbody>
        {{#users}}
          <tr>
            <td>{{username}}</td>
            <td>{{password}}</td>
            <td>{{level}}</td>
            <td><a href='editors/deleteUser?username={{username}}&password={{password}}'>D</a></td>
          </tr>
        {{/users}}
      </tbody>
    </table>

    <h4>Articles</h4>
    <table>
      <thead>
        <tr>
          <th>Title</th>
          <th>Author</th>
          <th>Delete</th>
        </tr>
      </thead>
      <tbody>
        {{#articles}}
          <tr>
            <td>{{title}}</td>
            <td>{{username}}</td>
            <td><a href='editors/deleteArticle?title={{title}}&author={{username}}'>D</a></td>
          </tr>
        {{/articles}}
      </tbody>
    </table>
  </div>

  {{>sidebar}}

</div>
